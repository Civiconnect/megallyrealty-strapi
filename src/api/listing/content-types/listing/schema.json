{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listings",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Price": {
      "type": "biginteger",
      "required": true
    },
    "Bedrooms": {
      "type": "decimal",
      "required": true
    },
    "Bathrooms": {
      "type": "decimal",
      "required": true
    },
    "SqFt": {
      "type": "biginteger",
      "required": true
    },
    "FeaturedListing": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "FeaturedPhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "PhotosAndVideos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "location": {
      "type": "customField",
      "customField": "plugin::location-field.location"
    },
    "MLSLink": {
      "type": "string",
      "required": true
    },
    "Availability": {
      "type": "enumeration",
      "enum": [
        "For Sale",
        "Sold",
        "For Lease",
        "Leased"
      ],
      "required": true,
      "default": "For Sale"
    },
    "MLS": {
      "type": "string"
    },
    "Address": {
      "type": "string",
      "private": true
    }
  }
}
